#include <stdio.h>

int factorial (int n){
  int result = 1;
  
  for(int i = 1; i<=n; i++){
    result *= i;
  }
  
  return result;
  
}

int main(){
  int num;
  
  printf("enter a number:");
  scanf("%d", &num);
  
  int fact = factorial(num);
  
  printf("the factorial of %d s %d\n", num, fact);
  
  return 0;
}
